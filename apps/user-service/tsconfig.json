{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "@mora/tsconfig/services.json",
    "compilerOptions": {
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "module": "commonjs"
    },
    "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.js", "src/**/*.jsx", 
    "../../packages/schemas/src/**.ts",
    "../../packages/utils/src/**.ts", "../../packages/schemas/src/user-service/user-service.schema.ts", "../../packages/schemas/src/repo-service/repo-service.schema.ts", "../../packages/schemas/src/auth-services/auth-service.schema.ts",
    ],
    "exclude": ["node_modules", "dist", "build", "coverage"],
}